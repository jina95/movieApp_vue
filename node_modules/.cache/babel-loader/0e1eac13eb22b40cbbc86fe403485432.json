AAA,CAAA,QAAA,CAAA,YAAA,YAAA,KAAA,YAAA,cAAA,KAAA,KAAA;AACA,GArBA;AAsBA,EAAA,OAAA,EAAA;AACA,IAAA,WADA,uBACA,IADA,EACA;AAAA;AACA;AACA,aAAA,IAAA,CAAA,OAAA,CAAA,aAAA,EAAA,EAAA,CAAA;AACA,KAJA;AAKA,IAAA,QALA,oBAKA,IALA,EAKA;AACA;AACA,UAAA,IAAA,CAAA,QAAA,CAAA,GAAA,CAAA,EAAA;AACA;AACA,eAAA,IAAA,CAAA,SAAA,CAAA,CAAA,EAAA,IAAA,CAAA,OAAA,CAAA,GAAA,CAAA,CAAA,CAFA,CAGA;AACA;AACA,OALA,MAKA,IAAA,IAAA,KAAA,EAAA,EAAA;AACA;AACA,eAAA,6BAAA;AACA,OAHA,MAGA,OAAA,IAAA,CAVA,CAWA;;AACA,KAjBA;AAkBA,IAAA,aAlBA,yBAkBA,IAlBA,EAkBA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA,EADA,CAEA;;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,gBAAA,EAAA,IAAA,EAHA,CAIA;AACA;AACA;AAxBA;AAtBA,CAAA","sourcesContent":["<template>\n  <div class=\"resultForm\">\n    <span>\n      다음과 관련된 콘텐츠 :\n      <b>{{ value }}</b>\n    </span>\n    <span class=\"resultTotal\">토탈 값: {{ resultTotalCount }}</span>\n    <ul>\n      <li v-for=\"item in $store.state.movie\" :key=\"item.DOCID\">\n        <h3>{{ replaceName(item.title) }}</h3>\n        <a ><img :src=\"srcCheck(item.posters)\"/></a>\n        <!-- <router-link to=\"/infor\"></router-link> -->\n        <div class=\"movieInfor\" @click.prevent=\"goToInfroPage(item)\">\n          <span>감독: {{ item.directors.director[0].directorNm }}</span>\n          <p>{{ item.plots.plot[0].plotText }}</p>\n        </div>\n      </li>\n    </ul>\n  </div>\n</template>\n\n<script>\n// import { eventBus } from '../main';\nimport { log } from 'util';\nexport default {\n  data() {\n    return {\n      value: this.$store.state.value,\n      selectedType: this.$store.state.type,\n      titleName:''\n    };\n  },\n  computed: {\n    resultMovie() {\n      return this.$store.state.movie;\n    },\n    resultTotalCount() {\n      return this.$store.state.count;\n    },\n  },\n  created() {\n    // eventBus.$on('loadMovie', inputValue => {\n    //   this.value = inputValue;\n    // });\n    this.$store.dispatch('FECH_MOVIE', `${this.selectedType}=${this.value}`);\n  },\n  methods: {\n    replaceName(name){ // 검색시 뜨는 !HS 와 !HE 제거 \n      // this.titleName = name.replace(/!HS|!HE|\\s/g, '')\n      return name.replace(/!HS|!HE|\\s/g, '')\n    },\n    srcCheck(item) {\n      // poster 주소를 인자로 받아와서\n      if (item.includes('|')) {\n        //|가 포함되있다면(중복될때 | 가 들어감)\n        return item.substring(0, item.indexOf('|'));\n        // substring 으로 0번째 글자부터 |의 위치를 받아와서 자른다.\n        // substring([시작위치], [종료위치]);\n      } else if (item === '') {\n        // 포스터 주소가 비어져 있을때\n        return 'http://placehold.it/213x316';\n      } else return item;\n      // | 를 포함하지 않는다는 것은 주소가 중복이 아니기 때문에 리턴해준다.\n    },\n    goToInfroPage(item){\n      console.log(item)\n      // this.gotoMovie = item;\n      this.$store.commit('SET_INFORMOVIE', item )\n      // this.$store.commit('SET_INFORTITLE',  )\n      // this.$router.push('/information')\n    }\n  },\n};\n</script>\n\n<style></style>\n"],"sourceRoot":"src/components"}]}